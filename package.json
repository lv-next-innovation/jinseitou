{
  "name": "jinseitou",
  "private": true,
  "scripts": {
    "start:js": "parcel watch src/app.tsx -d public --hmr-port 8080 --hmr-hostname 0.0.0.0 --log-level 4",
    "start:css": "parcel watch src/style/style.scss -d public --hmr-port 8000 --hmr-hostname 0.0.0.0 --log-level 4",
    "build": "parcel build src/app.tsx -d public && parcel build src/style/style.scss -d public",
    "tslint": "tslint --fix 'src/**/*.ts'",
    "prettier": "prettier --write src/**/*.{ts,tsx}"
  },
  "dependencies": {
    "prettier": "^1.17.1",
    "react": "16.x",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.18",
    "@fortawesome/free-solid-svg-icons": "^5.8.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@types/react": "^16.8.17",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.0.9",
    "@types/redux": "^3.6.0",
    "parcel-bundler": "^1.12.3",
    "sass": "^1.20.1",
    "ts-loader": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1"
  }
}
